pipeline {
    agent any

    stages {
        stage('SCM checkout') {
            steps {
                echo 'Getting from Github' 
                echo 'SCM checkout completed...'
            }
        }
        stage('Build') {
            steps {
                echo 'Build completed...'
            }
        }
        stage('Test') {
            steps {
                echo 'Test completed...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy completed...'
            }
        }
    }
}
